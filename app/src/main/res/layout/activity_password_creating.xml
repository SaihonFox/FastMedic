    <Button
        android:id="@+id/ButtonDelete_pc"
        android:layout_width="35dp"
        android:layout_height="24dp"
        android:layout_marginStart="230dp"
        android:layout_marginTop="340dp"
        android:background="@android:color/transparent"
        android:foreground="@drawable/del_icon"
        app:layout_constraintBottom_toBottomOf="@id/keyboard_pc"
        app:layout_constraintEnd_toEndOf="@id/keyboard_pc"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@id/keyboard_pc"
        app:layout_constraintTop_toTopOf="@id/keyboard_pc"
        app:layout_constraintVertical_bias="0.0"
