        android:layout_height="80dp"
        android:layout_marginTop="104dp"
        app:layout_constraintBottom_toBottomOf="@id/keyboard_pc"
        app:layout_constraintEnd_toEndOf="@id/keyboard_pc"
        app:layout_constraintStart_toStartOf="@id/keyboard_pc"
        app:layout_constraintTop_toTopOf="@id/keyboard_pc"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:stateListAnimator="@null"
        android:id="@+id/Button4_pc"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/custom_oval"
        android:text="4"
        android:textSize="24sp"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@id/keyboardRow2_pc"
        app:layout_constraintEnd_toEndOf="@id/keyboardRow2_pc"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@id/keyboardRow2_pc"
        app:layout_constraintTop_toTopOf="@id/keyboardRow2_pc"
        tools:ignore="HardcodedText" />

    <Button
        android:stateListAnimator="@null"
