<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.FastMedic.Custom"
        tools:targetApi="31">
        <activity
            android:name=".ShoppingCartActivity"
            android:exported="false" />
        <activity
            android:name=".AnalysesActivity"
            android:exported="false" />
        <activity
            android:name=".LoginAndRegistrationActivity"
            android:exported="false" />
        <activity
            android:name=".CardCreatingActivity"
            android:exported="false" />
        <activity
            android:name=".PasswordCreatingActivity"
            android:exported="false" />
        <activity
            android:name=".EmailCodeActivity"
            android:exported="false" />
        <activity
            android:name=".OnboardActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.FastMedic.Custom.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>